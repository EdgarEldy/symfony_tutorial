{% extends('default/base.html.twig') %}
{% block content %}
    <div class="col-lg-12">
        <div class="panel panel-default">
            <div class="panel-heading">Edit a category</div>
            <div class="panel-body">
                <div class="col-md-6">
                    {{ form_start(form) }}
                    {{ form_widget(form) }}
                    <button class="btn btn-primary">{{ button_label|default('Save') }}</button>
                    {{ form_end(form) }}
                </div>
            </div>
        </div>
    </div><!-- /.col-->
{% endblock %}